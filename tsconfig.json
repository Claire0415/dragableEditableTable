{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "jsx": "react",
    "sourceMap": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
     "lib": [
      "dom",
      "es5",
      "es6"
    ]
  },
  "compileOnSave": false,
  "exclude": ["node_modules"]
}
